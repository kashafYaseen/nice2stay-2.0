<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>properties</th>
      <th>Time</th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.properties %></td>
        <td><%= render_datetime event.time %></td>
      </tr>
    <% end %>
  </tbody>
</table>
